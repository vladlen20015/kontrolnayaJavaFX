      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="288.0" prefWidth="270.0">
         <children>
            <Label layoutX="37.0" layoutY="51.0" text="login" />
            <Label layoutX="25.0" layoutY="91.0" text="password" />
            <TextField fx:id="login" layoutX="114.0" layoutY="47.0" prefHeight="25.0" prefWidth="109.0" />
            <TextField fx:id="password" layoutX="114.0" layoutY="87.0" prefHeight="25.0" prefWidth="109.0" />
